{
  "manifest_version": 2,

  "name": "Chrome Violently Rejected",
  "description": "Changes Facebook 'Seen' to 'Violently rejected'",
  "version": "1.0",

  "permissions": [
    "https://facebook.com/",
    "http://facebook.com/",
    "https://www.facebook.com/",
    "http://www.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "js": ["jquery-1.11.0.min.js", "myscript.js"]
    }
  ]
}